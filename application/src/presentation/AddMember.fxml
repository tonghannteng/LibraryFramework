<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="490.0" prefWidth="468.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.AddMember">
   <children>
      <VBox prefHeight="81.0" prefWidth="547.0" style="-fx-background-color: EE2E12;">
         <children>
            <Label alignment="CENTER" prefHeight="56.0" prefWidth="547.0" text="Add Library Member to System" textFill="WHITE">
               <font>
                  <Font name="Corbel Bold" size="19.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <HBox alignment="CENTER" prefHeight="68.0" prefWidth="547.0">
         <children>
            <TextField fx:id="txtmember_id" prefHeight="40.0" prefWidth="171.0" promptText="Member ID" styleClass="textfield" stylesheets="@st.css">
               <HBox.margin>
                  <Insets left="5.0" right="5.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="66.0" prefWidth="547.0">
         <children>
            <TextField fx:id="txtFirstName" prefHeight="48.0" promptText="Last Name" stylesheets="@st.css" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="66.0" prefWidth="547.0">
         <children>
            <TextField fx:id="txtLastName" promptText="Last Name" stylesheets="@st.css" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="66.0" prefWidth="547.0">
         <children>
            <TextField fx:id="txtStreet" promptText="Street" stylesheets="@st.css" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="66.0" prefWidth="200.0">
         <children>
            <TextField fx:id="txtCity" promptText="City" stylesheets="@st.css" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="66.0" prefWidth="200.0">
         <children>
            <TextField fx:id="txtState" promptText="State" stylesheets="@st.css" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="66.0" prefWidth="200.0">
         <children>
            <TextField fx:id="txtZip" promptText="zip" stylesheets="@st.css" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="66.0" prefWidth="414.0">
         <children>
            <TextField fx:id="txtPhone" promptText="Phone Number" stylesheets="@st.css" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button mnemonicParsing="false" onAction="#handleSubmitButtonAction" prefHeight="36.0" prefWidth="115.0" stylesheets="@st.css" text="Submit" textFill="WHITE">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
               <font>
                  <Font name="Corbel Bold" size="14.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#handleResetButtonAction" prefHeight="36.0" prefWidth="115.0" stylesheets="@st.css" text="Reset" textFill="WHITE">
               <font>
                  <Font name="Corbel Bold" size="14.0" />
               </font></Button>
         </children>
      </HBox>
      <Pane prefHeight="0.0" prefWidth="541.0" style="-fx-background-color: EE2E12;" />
   </children>
</VBox>
